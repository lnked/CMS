{
  "name": "fastest/fastest",
  "type": "project",
  "description": "Fast PHP CMS",
  "license" : "BSD-3-Clause",
  "authors": [
    {
      "name": "Edik Bulikyan",
      "email": "ed.proff@gmail.com",
      "role": "Developer"
    }
  ],
  "support" : {
    " issues" : "https://github.com/lnked/fastest/issues"
  },
  "require": {
    "php": "7.1.* || 7.2.*",
    "badoo/liveprof": "^1.0",
    "codeguy/upload": "^1.3",
    "intervention/image": "^2.3",
    "jenssegers/imagehash": "^0.6.0",
    "kriswallsmith/assetic": "^1.4",
    "league/flysystem": "^1.0",
    "lnked/pubsub": "dev-master",
    "lnked/q": "dev-master",
    "matthiasmullie/minify": "^1.3",
    "phpfastcache/phpfastcache": "^7.0",
    "phpoffice/phpspreadsheet": "^1.1",
    "prewk/xml-streamer": "^0.5.0",
    "react/http": "^0.8.4",
    "samdark/sitemap": "^2.0",
    "smarty/smarty": "^3.1",
    "spomky-labs/base64url": "^1.0",
    "swiftmailer/swiftmailer": "^6.0",
    "twig/twig": "^2.4",
    "vlucas/phpdotenv": "^2.4",
    "voronkovich/sberbank-acquiring-client": "^1.1",
    "wapmorgan/unified-archive": "^0.1.2",
    "xobotyi/php-mime-type": "^1.2",
    "zircote/swagger-php": "^2.0"
  },
  "require-dev": {
    "paquettg/php-html-parser": "^1.7"
  },
  "config": {
    "optimize-autoloader": true,
    "platform": {
      "php": "7.1"
    },
    "preferred-install": "dist",
    "sort-packages": true,
    "vendor-dir": "protected/app/vendors"
  },
  "autoload": {
    "psr-4": {
      "Acme\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Acme\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
